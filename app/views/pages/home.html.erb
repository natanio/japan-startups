  <!-- Features Section -->
  <div id="home" class="section welcome">
    <img class="background" src="https://s3.amazonaws.com/japanstartups/img/mt-fuji.jpg" alt="stream blurred">
    <div class="container">
      <div class="content">
        <span class="logo">
          <%= image_tag 'https://s3.amazonaws.com/japanstartups/img/logo.png' %>
        </span>
        <h1>Connecting Innovative Minds in Japan</h1>
        <a class="scroll-down btn" href="#about">LEARN MORE</a>
        <a class="scroll-down" href="#about"><i class="fa fa-angle-down"></i></a>
      </div><!-- /.content -->
